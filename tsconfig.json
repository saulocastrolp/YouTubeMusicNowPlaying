{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["esnext"],
        "jsx": "react-native",
        "allowJs": true,
        "skipLibCheck": true,
        "noEmit": true,
        "isolatedModules": true,
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "*": ["src/*"]
        }
    }
}
